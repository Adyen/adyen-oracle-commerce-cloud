{
  "name": "adyen-occ-server",
  "version": "1.0.0",
  "main": "./app/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rollup --config config/config.dev.js",
    "server": "nodemon app/bundle.js",
    "lint": "prettier --write 'src/**/*.js'",
    "watch": "concurrently --kill-others \"npm run dev -- --watch\" \"npm run server\"",
    "build": "yarn run lint && rollup --config config/config.prod.js",
