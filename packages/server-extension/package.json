  ],
  "author": "Ricardo Ambrogi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Adyen/adyen-oracle-commerce-cloud/issues"
  },
  "homepage": "https://github.com/Adyen/adyen-oracle-commerce-cloud#readme",
  "dependencies": {
    "@adyen/api-library": "^2.1.6",
    "form-data": "^2.5.1",
    "memory-cache": "^0.2.0",
    "ua-parser-js": "^0.7.20"
  },
  "devDependencies": {
    "body-parser": "latest",
    "express": "latest",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "nconf": "^0.10.0",
    "nodemon": "^1.19.1",
    "winston": "latest"
  },
  "description": "",
